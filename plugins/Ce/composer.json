{
    "name": "pluginception/ce",
    "description": "Root CE plugin for Pluginception",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Pluginception\\Ce\\": "src/",
            "Pluginception\\Ce\\Log\\": "plugins/Log/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pluginception\\Ce\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "Pluginception\\Ce\\Log\\Test\\": "plugins/Log/tests/"
        }
    }
}
